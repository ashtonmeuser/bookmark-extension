{
  "manifest_version": 3,
  "name": "Omni Bookmark",
  "description": "Easily search and open bookmarks",
  "version": "1.0",
  "permissions": ["bookmarks", "scripting", "activeTab", "storage"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": "icon.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      }
    }
  }
}
